{
  "expo": {
    "owner": "mikuaiza69",
    "name": "NeuroConnect Zone",
    "slug": "neuroconnect-zone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/NCZ_LOGO2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/NCZ_LOGO2.png",
      "resizeMode": "contain",
      "backgroundColor": "#4CC9FE"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/NCZ_LOGO2.png",
        "backgroundColor": "#4CC9FE"
      },
      "package": "com.aizal.neuroconnectzone",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/NCZ_LOGO2.png"
    },
    "cli": {
      "appVersionSource": "remote"
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Allow NeuroConnect Zone to access your microphone."
        }
      ]
    ],
    "sdkVersion": "52.0.0",
    "extra": {
      "eas": {
        "projectId": "neuroconnect-zone"
      }
    }
  }
}
